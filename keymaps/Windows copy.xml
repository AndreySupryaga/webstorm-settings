<keymap version="1" name="Windows copy" parent="$default">
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl w" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="ctrl s" />
  </action>
  <action id="EditorUnSelectWord">
    <keyboard-shortcut first-keystroke="shift ctrl w" />
    <keyboard-shortcut first-keystroke="shift ctrl s" />
  </action>
  <action id="ExportSettings">
    <keyboard-shortcut first-keystroke="shift ctrl alt e" />
  </action>
  <action id="SaveAll" />
  <action id="SaveAs" />
</keymap>