<templateSet group="Zen HTML">
  <template name="pr" value="&lt;pre&gt;{{$END$ | json}}&lt;/pre&gt;" description="Angular template json" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="for" value="@for (item of $VAR1$; track item) {&#10;    $END$&#10;}" shortcut="TAB" description="*ngFor=&quot;let $VAR0$ of $VAR1$&quot; with div" toReformat="true" toShortenFQNames="true">
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="nc" value="&lt;ng-container *ngIf=&quot;$VAR$&quot;&gt;&#10;    $END$&#10;&lt;/ng-container&gt;" shortcut="TAB" description="ng-content" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="nt" value="&lt;ng-template $VAR$&gt;&#10;    $END$&#10;&lt;/ng-template&gt;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="forr" value="*ngFor=&quot;let $VAR0$ of $VAR1$&quot;" shortcut="TAB" description="*ngFor=&quot;let $VAR0$ of $VAR1$&quot;" toReformat="true" toShortenFQNames="true">
    <variable name="VAR0" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VAR1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
  <template name="sc" value="&lt;ng-container [ngSwitch]=&quot;condition&quot;&gt;&#10;      &lt;ng-container *ngSwitchCase=&quot;case&quot;&gt;&#10;&#10;      &lt;/ng-container&gt;&#10;      &lt;ng-container *ngSwitchDefault&gt;&#10;          &#10;      &lt;/ng-container&gt;&#10;&lt;/ng-container&gt;" shortcut="TAB" description="ngSwitch" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
    </context>
  </template>
</templateSet>